{
  "name": "@accesso/frontend",
  "version": "0.2.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 HTTPS=true CLIENT_PUBLIC_PATH=https://localhost:3001 razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "format": "prettier --write \"./src/**/**.{ts,tsx,js,jsx,json}\"",
    "commit": "git-cz"
  },
  "engines": {
    "node": "^v14.14.0"
  },
  "dependencies": {
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.5",
    "cross-fetch": "^3.0.4",
    "effector": "^21.5.0",
    "effector-inspector": "^0.4.1",
    "effector-logger": "^0.9.0",
    "effector-react": "^21.0.6",
    "effector-root": "^1.1.0",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "http-proxy-middleware": "^1.0.3",
    "node-fetch": "^2.6.0",
    "patronum": "0.103.1",
    "query-string": "^6.11.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.0.1",
    "styled-components": "^5.0.1",
    "typed-contracts": "^2.3.0",
    "universal-fetch": "^1.0.0",
    "woly": "0.1.10"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@svgr/webpack": "^5.2.0",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.5",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^23.3.14",
    "@types/node": "^14.14.2",
    "@types/node-fetch": "^2.5.5",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^5.0.1",
    "@types/webpack-env": "^1.15.3",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "^3.1.0",
    "jest": "^26.6.1",
    "lint-staged": "^9.5.0",
    "prettier": "^2.0.4",
    "razzle": "^3.0.0",
    "razzle-plugin-typescript": "^3.0.0",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3",
    "typescript-styled-plugin": "^0.15.0"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": ["ts", "tsx", "js", "json"],
    "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}"]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
